<UserControl x:Class="TMT.iAdjudicate.cPictureDisplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="234" d:DesignWidth="300">
    <Grid VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <Border Name="bdrImage" CornerRadius="10" BorderThickness="0" ClipToBounds="True">
            <Image Name="mImage" Opacity="1" RenderTransformOrigin="0.5,0.5" />
        </Border>
    </Grid>
</UserControl>
