

<script>
    $("#breadcrumbText").html("TIS / <b>Import TIS Details</b>");
</script>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Import TIS Details</h5>
                </div>
                <div class="ibox-content">
                    @using (Html.BeginForm("ImportTISDetails", "TISCapture", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {
                    @Html.AntiForgeryToken()
                    
                        <div class="form-horizontal">                        
                            <div class="form-group">
                                @Html.Label("CSV File", new { @class = "control-label col-sm-2" })
                                <div class="col-sm-4">
                                    <input type="file" id="csvFile" class="form-control" accept=".csv" name="csv" />
                                </div>
                                @*<button dir="auto" id="search" onclick="findCsvPath"> Browse </button>*@
                            </div>
                            <div class="form-group">
                                <div class="col-md-offset-2 col-md-10">
                                    <input type="submit" value="Import" class="btn btn-primary" />       
                                </div>                        
                            </div>
                        </div>
                  }
                </div>
            </div>
        </div>
    </div>
</div>
